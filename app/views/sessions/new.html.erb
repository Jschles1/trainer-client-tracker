<h1>Log In:</h1>
<br></br>
<%= form_for @user, :url => url_for(:controller => 'sessions', :action => 'create') do |f| %>
  <%= f.label :email %>
  <br></br>
  <%= f.text_field :email %>
  <br></br>
  <%= f.label :password %>
  <br></br>
  <%= f.password_field :password %>
  <br></br>
  <%= f.submit "Log In"%>
<% end %>
